@prefix ub:     <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#> .
@prefix sh:     <http://www.w3.org/ns/shacl#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
        
ub:Professor
	a sh:NodeShape ;
	sh:targetClass ub:FullProfessor ;
	sh:property [
		sh:path ub:name ;
		sh:maxLength 10 ;
	] .